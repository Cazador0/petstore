{
  "name": "@morty/web",
  "version": "0.1.0",
  "tasks": {
    "dev": "deno run -A --unstable-byonm npm:next dev --turbopack -p 9002",
    "build": "deno run -A --unstable-byonm npm:next build",
    "start": "deno run -A --unstable-byonm npm:next start",
    "lint": "deno run -A --unstable-byonm npm:next lint",
    "genkit:dev": "deno run -A --unstable-byonm npm:genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "deno run -A --unstable-byonm npm:genkit start -- tsx --watch src/ai/dev.ts",
    "server": "deno run -A --unstable-byonm server.ts",
    "init-db": "deno run -A --unstable-byonm init-db.ts",
    "test-api": "deno run -A --unstable-byonm test-api.ts"
  },
  "imports": {
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "next": "npm:next@15.3.3",
    "lucide-react": "npm:lucide-react@^0.475.0",
    "zod": "npm:zod@^3.24.2",
    "genkit": "npm:genkit@^1.20.0",
    "genkit-cli": "npm:genkit-cli@^1.20.0",
    "firebase": "npm:firebase@^11.9.1"
  },
  "exclude": [
    ".next",
    "node_modules",
    "out"
  ]
}
